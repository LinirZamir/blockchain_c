{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-g",
				"blockchain.c",
				"blockchain.utils.c",
				"data_containers/dict.c",
				"data_containers/linked_list.c",
				"nanomsg/lib/libnanomsg.a",
				"-o",
				"blockchain",
				"-lcrypto",
				"-lanl",
				"-pthread"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/gcc"
		}
	]
}